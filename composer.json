{
    "name": "vaslv/go-unisender-notifications-channel",
    "description": "GO Unisender Notifications Channel for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "peterls",
            "email": "support@web-kiborg.ru"
        }
    ],
    "require": {
      "php" : ">=7.1",
      "illuminate/support" : "*",
      "illuminate/notifications" : "*",
      "guzzlehttp/guzzle" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^1.0"
    },
  "autoload" : {
    "psr-4" : {
      "NotificationChannels\\GoUnisender\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "NotificationChannels\\GoUnisender\\Tests\\" : "tests/"
    }
  },
  "minimum-stability" : "dev",
  "extra" : {
    "laravel" : {
      "providers" : [
        "NotificationChannels\\GoUnisender\\Providers\\GoUnisenderChannelServiceProvider"
      ]
    }
  }
}
